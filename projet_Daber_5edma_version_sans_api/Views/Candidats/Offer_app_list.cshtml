@model IEnumerable<projet_Daber_5edma_version_sans_api.Models.Candidat_Application>

@{
    ViewData["Title"] = "Offer_app_list";
}

<h1>Offer_app_list</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Speciality)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameCompany)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelCompany)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailCompany)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Speciality)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NameCompany)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelCompany)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailCompany)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
        </tr>
}
    </tbody>
</table>
